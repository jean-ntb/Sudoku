<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data_File\duplicate_value.data" />
    <None Remove="Data_File\easy.data" />
    <None Remove="Data_File\hard.data" />
    <None Remove="Data_File\invalid_board.data" />
    <None Remove="Data_File\valid_board.data" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Data_File\duplicate_value.data">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Data_File\easy.data">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Data_File\hard.data">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Data_File\invalid_board.data">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Data_File\valid_board.data">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
